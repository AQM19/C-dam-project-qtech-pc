<Window x:Class="Q_Tech.Modales.FrmDashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Q_Tech.Modales"
        mc:Ignorable="d"
        Title="FrmDashboard" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="/Recursos/Fondos/login_green_background.jpeg"/>
    </Window.Background>
    <Grid>
        <DockPanel LastChildFill="True">
            <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Height="auto" Width="120">
                <TextBlock Text="Q-TECH" TextAlignment="Center" FontSize="25" Padding="0, 15"/>
                <Button>
                    <Image Source="" />
                </Button>
            </StackPanel>
        </DockPanel>
    </Grid>
</Window>
